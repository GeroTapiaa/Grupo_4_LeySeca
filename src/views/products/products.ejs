<!DOCTYPE html>
<html lang="en">
<%- include('../partials/head', { title : 'Productos' }) %>

  <body class="home">
    <%- include('../partials/modal') %> <%- include('../partials/navbar') %>

        <!-- MAIN -->

        <main class="home__main">
          <section class="section">
            <img src="/images/logos/bg-beer (1).png" width="100%" alt="" />
          </section>

          <!-- MAIN SECTION PRODUCTS -->

          <h1 id="title_shop">Nuestros Productos</h1>

          <section class="home__main__section__products">
            <% productsBeer.forEach(({id, nameProduct, price , discount, image})=> { %>
              <%- include('../partials/products',{ id, nameProduct, price, discount, image, toThousand }) %>
                <% }) %>
          </section>
        </main>

        <%- include('../partials/footer') %>
  </body>

</html>